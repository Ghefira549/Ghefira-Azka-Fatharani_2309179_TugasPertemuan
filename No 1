package pkg2309179_ghefira.azka.fatharani_tugas4_no1;

/**
 *
 * @author FARIZ
 */
import java.util.Scanner;
public class AzkaFatharani_tugas4_no1 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        System.out.println("No. 1 menampilkan Bilangan 1-n");
        Scanner inputObjk = new Scanner(System.in);
        System.out.print("Masukkan Batasan Angka : ");
        int n=inputObjk.nextInt();
        for(int i=1 ; i<=n; i++){
            System.out.println(i); 
        }
        
        
        
        
        
    }
    
}
